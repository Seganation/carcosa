{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "postinstall": "echo 'skip prisma generate'",
    "check-types": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "db:generate": "npm run --workspace @carcosa/database db:generate",
    "db:migrate": "npm run --workspace @carcosa/database db:migrate",
    "db:deploy": "npm run --workspace @carcosa/database db:deploy",
    "db:push": "npm run --workspace @carcosa/database db:push",
    "db:seed": "npm run --workspace @carcosa/database db:seed"
  },
  "dependencies": {
    "@carcosa/database": "*",
    "@carcosa/file-router": "^0.1.0",
    "@carcosa/storage": "*",
    "@carcosa/types": "*",
    "@types/bcryptjs": "^2.4.6",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-fileupload": "^1.5.2",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "libsodium-wrappers": "^0.7.14",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.13.1",
    "sharp": "^0.33.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-fileupload": "^1.5.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.3",
    "@types/pg": "^8.11.10",
    "eslint": "^9.31.0",
    "tsx": "^4.19.2",
    "typescript": "5.8.3"
  }
}
