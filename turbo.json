{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "@repo/typescript-config#build": {
      "cache": false
    },
    "@repo/eslint-config#build": {
      "cache": false
    },
    "@carcosa/types#build": {
      "outputs": ["dist/**"]
    },
    "@carcosa/storage#build": {
      "dependsOn": ["@carcosa/types#build"],
      "outputs": ["dist/**"]
    },
    "@carcosa/database#build": {
      "outputs": ["dist/**"]
    },
    "@carcosa/sdk#build": {
      "dependsOn": ["@carcosa/types#build"],
      "outputs": ["dist/**"]
    },
    "@carcosa/file-router#build": {
      "dependsOn": ["@carcosa/types#build", "@carcosa/storage#build"],
      "outputs": ["dist/**"]
    },
    "@carcosa/nextjs#build": {
      "dependsOn": ["@carcosa/types#build", "@carcosa/sdk#build"],
      "outputs": ["dist/**"]
    },
    "@carcosa/cmage#build": {
      "dependsOn": ["@carcosa/types#build", "@carcosa/sdk#build"],
      "outputs": ["dist/**"]
    },
    "@carcosa/ui#build": {
      "cache": false
    },
    "web#build": {
      "dependsOn": ["^build", "@carcosa/database#db:generate"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"],
      "env": ["DATABASE_URL"]
    },
    "test#build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "api#build": {
      "dependsOn": ["^build", "@carcosa/database#db:generate"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"],
      "env": ["DATABASE_URL"]
    },
    "docs#build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "web#dev": {
      "dependsOn": ["@carcosa/database#db:generate"],
      "cache": false,
      "persistent": true
    },
    "test#dev": {
      "cache": false,
      "persistent": true
    },
    "api#dev": {
      "dependsOn": ["@carcosa/database#db:generate"],
      "cache": false,
      "persistent": true
    },
    "docs#dev": {
      "cache": false,
      "persistent": true
    },
    "web#lint": {
      "dependsOn": ["^build"]
    },
    "test#lint": {
      "dependsOn": ["^build"]
    },
    "api#lint": {
      "dependsOn": ["^build"]
    },
    "docs#lint": {
      "dependsOn": ["^build"]
    },
    "web#check-types": {
      "dependsOn": ["^build"]
    },
    "test#check-types": {
      "dependsOn": ["^build"]
    },
    "api#check-types": {
      "dependsOn": ["^build"]
    },
    "docs#check-types": {
      "dependsOn": ["^build"]
    },
    "@carcosa/database#db:generate": {
      "cache": false
    },
    "@carcosa/database#db:migrate": {
      "cache": false,
      "persistent": true
    },
    "@carcosa/database#db:deploy": {
      "cache": false
    }
  }
}
