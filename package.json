{
  "name": "carcosa",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@vitest/ui": "^2.1.4",
    "prettier": "^3.6.2",
    "turbo": "^2.5.5",
    "typescript": "5.8.3",
    "vitest": "^2.1.4"
  },
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@11.4.1",
  "workspaces": [
    "apps/*",
    "apps/web/*",
    "packages/*",
    "!packages/_archived-*"
  ],
  "dependencies": {
    "@prisma/client": "^6.14.0"
  }
}
