{
  "name": "@carcosa/sdk",
  "version": "0.1.0",
  "type": "module",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "default": "./dist/react/index.js"
    },
    "./nextjs": {
      "types": "./dist/nextjs/index.d.ts",
      "default": "./dist/nextjs/index.js"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "default": "./dist/cli/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "check-types": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "test": "vitest run"
  },
  "dependencies": {
    "@carcosa/types": "*",
    "commander": "^12.1.0"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "next": "^13.0.0 || ^14.0.0 || ^15.0.0"
  },
  "peerDependenciesMeta": {
    "react": { "optional": true },
    "react-dom": { "optional": true },
    "next": { "optional": true }
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "typescript": "5.8.3",
    "vitest": "^2.1.4",
    "@types/node": "^22.15.3",
    "eslint": "^9.31.0"
  },
  "description": "Official Carcosa SDK for file uploads and transformations",
  "author": "Carcosa Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/Seganation/carcosa.git",
    "directory": "packages/sdk"
  },
  "homepage": "https://github.com/Seganation/carcosa#readme",
  "bugs": {
    "url": "https://github.com/Seganation/carcosa/issues"
  },
  "keywords": ["carcosa", "upload", "file-upload", "sdk", "client"]
}

