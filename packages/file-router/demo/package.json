  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "tsx": "^4.19.2",
    "vite": "^5.0.0",
    "typescript": "5.8.3",
    "concurrently": "^8.2.0"
  },
  "keywords": [
    "file-upload",
    "uploadthing",
    "demo",
    "real-time",
    "multi-storage",
    "typescript"
