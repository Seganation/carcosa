{
  "name": "@carcosa/file-router-demo",
  "version": "1.0.0",
  "description": "Live demo of Carcosa File Router - The UploadThing Killer",
  "private": true,
  "type": "module",
  "scripts": {
    "demo:api": "tsx api-demo.ts",
    "demo:react": "vite react-demo.tsx --port 3001",
    "demo:build": "tsc --noEmit",
