{
  "name": "@carcosa/nextjs",
  "version": "0.1.0",
  "type": "module",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "default": "./dist/hooks/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "default": "./dist/components/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "default": "./dist/utils/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "check-types": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "test": "vitest run"
  },
  "dependencies": {
    "@carcosa/sdk": "*",
    "@carcosa/types": "*",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "next": "^13.0.0 || ^14.0.0 || ^15.0.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "typescript": "5.8.3",
    "vitest": "^2.1.4",
    "eslint": "^9.31.0"
  },
  "description": "Next.js integration utilities for Carcosa",
  "author": "Carcosa Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/Seganation/carcosa.git",
    "directory": "packages/nextjs"
  },
  "homepage": "https://github.com/Seganation/carcosa#readme",
  "bugs": {
    "url": "https://github.com/Seganation/carcosa/issues"
  },
  "keywords": ["carcosa", "nextjs", "next", "react", "integration", "hooks"]
}
